      re := real(x); im := imag(x)
      zero? re and im > 0 => [imaginary(), complex(im,0), -imaginary()]
      re > 0 =>
        im >= 0 => [1, x, 1]
        im < 0 => [-imaginary(), complex(-im, re), imaginary()]
      im < 0 => [-1, complex(-re, -im) ,-1]
      [imaginary(), complex(im, -re), -imaginary()]

