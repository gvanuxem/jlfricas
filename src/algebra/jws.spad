    tan(z) == junfunc("W_"Tan_"(",z)
    sec(z) == junfunc("W_"Sec_"(",z)
    csc(z) == junfunc("W_"Csc_"(",z)
    cot(z) == junfunc("W_"Cot_"(",z)

    sinc(z)== junfunc("W_"Sinc_"(",z)

    asin(z)   == junfunc("W_"ArcSin_"(",z)
    acos(z)   == junfunc("W_"ArcCos_"(",z)
